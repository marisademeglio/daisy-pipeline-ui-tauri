{
  "package": {
    "productName": "daisy-pipeline-prototype-tauri",
    "version": "0.3.0"
  },
  "build": {
    "distDir": "../docs",
    "devPath": "http://localhost:5555",
    "beforeDevCommand": "npm run serve-ui",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "org.daisy.pipeline.ui-prototype-tauri",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "notification": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "DAISY Pipeline",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false,
        "visible": true
      },

      {
        "title": "DAISY Pipeline Jobs",
        "width": 500,
        "height": 500,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "url": "jobs.html",
        "label": "jobs"
      }
    ],
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    }
  }
}